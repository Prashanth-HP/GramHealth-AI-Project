{
  "Dengue Fever": {
    "title_en": "Dengue Fever",
    "title_ta": "டெங்கு காய்ச்சல்",
    "description_en": "Mosquito-borne viral infection causing high fever and severe pain.",
    "description_ta": "கொசுக்களால் பரவும் வைரஸ் தொற்று. அதிக காய்ச்சல் மற்றும் கடுமையான வலியை ஏற்படுத்தும்.",
    "first_aid_en": [
      "Hydrate with ORS, tender coconut water, or Nilavembu Kashayam (herbal drink).",
      "Take Paracetamol only (No Aspirin).",
      "Rest completely."
    ],
    "first_aid_ta": [
      "ORS, இளநீர் அல்லது நிலவேம்பு கசாயம் குடிக்கவும்.",
      "பாராசிட்டமால் (Paracetamol) மட்டும் எடுக்கவும்.",
      "முழு ஓய்வு எடுக்கவும்."
    ],
    "when_to_see_doctor_en": "IMMEDIATELY if bleeding occurs.",
    "when_to_see_doctor_ta": "ரத்தப்போக்கு இருந்தால் உடனே மருத்துவரை அணுகவும்."
  },
  "Chikungunya": {
    "title_en": "Chikungunya",
    "title_ta": "சிக்குன்குனியா",
    "description_en": "Viral disease causing sudden fever and severe joint pain.",
    "description_ta": "திடீர் காய்ச்சல் மற்றும் மூட்டு வலியை ஏற்படுத்தும் வைரஸ் நோய்.",
    "first_aid_en": [
      "Apply cold compress or castor oil to joints for relief.",
      "Hydrate well.",
      "Take mild painkillers."
    ],
    "first_aid_ta": [
      "மூட்டுகளுக்கு குளிர்ந்த ஒத்தடம் அல்லது விளக்கெண்ணெய் தடவவும்.",
      "நிறைய தண்ணீர் குடிக்கவும்.",
      "வலி நிவாரணி எடுக்கவும்."
    ],
    "when_to_see_doctor_en": "If fever > 3 days.",
    "when_to_see_doctor_ta": "காய்ச்சல் 3 நாட்களுக்கு மேல் இருந்தால்."
  },
  "Malaria": {
    "title_en": "Malaria",
    "title_ta": "மலேரியா",
    "description_en": "Parasitic infection causing cycles of fever and chills.",
    "description_ta": "நடுக்கம் மற்றும் காய்ச்சலை ஏற்படுத்தும் ஒட்டுண்ணி தொற்று.",
    "first_aid_en": [
      "Keep the patient cool.",
      "Drink plenty of fluids.",
      "Use mosquito nets."
    ],
    "first_aid_ta": [
      "உடலை குளிர்ச்சியாக வைக்கவும்.",
      "நிறைய தண்ணீர் குடிக்கவும்.",
      "கொசுவலை உபயோகிக்கவும்."
    ],
    "when_to_see_doctor_en": "Immediately for blood test.",
    "when_to_see_doctor_ta": "ரத்த பரிசோதனைக்கு உடனே செல்லவும்."
  },
  "Filaria": {
    "title_en": "Lymphatic Filariasis",
    "title_ta": "யானைக்கால் நோய்",
    "description_en": "Parasitic disease causing swelling of legs (elephantiasis).",
    "description_ta": "கால்களில் வீக்கத்தை (யானைக்கால்) ஏற்படுத்தும் ஒட்டுண்ணி நோய்.",
    "first_aid_en": [
      "Wash swollen limb with soap and water daily.",
      "Keep the leg elevated.",
      "Exercise the limb."
    ],
    "first_aid_ta": [
      "வீங்கிய காலை தினமும் சோப்பு போட்டு கழுவவும்.",
      "காலை உயர்த்தி வைக்கவும்.",
      "பயிற்சி செய்யவும்."
    ],
    "when_to_see_doctor_en": "For DEC medication and management.",
    "when_to_see_doctor_ta": "மருந்து மற்றும் சிகிச்சைக்கு."
  },
  "Scrub Typhus": {
    "title_en": "Scrub Typhus",
    "title_ta": "ஸ்க்ரப் டைபஸ் (செள்ளு காய்ச்சல்)",
    "description_en": "Mite-borne disease causing fever and black scab.",
    "description_ta": "பூச்சி கடித்து பரவும் நோய். காய்ச்சல் மற்றும் கருப்பு வடு வரும்.",
    "first_aid_en": [
      "Check body for black scab.",
      "Take fever medication.",
      "Seek help immediately."
    ],
    "first_aid_ta": [
      "உடலில் கருப்பு வடு உள்ளதா என பார்க்கவும்.",
      "காய்ச்சல் மாத்திரை எடுக்கவும்.",
      "உதவி நாடவும்."
    ],
    "when_to_see_doctor_en": "Immediately if black scab found.",
    "when_to_see_doctor_ta": "கருப்பு வடு இருந்தால் உடனே."
  },
  "Japanese Encephalitis": {
    "title_en": "Japanese Encephalitis",
    "title_ta": "ஜப்பானிய மூளைக்காய்ச்சல்",
    "description_en": "Viral brain infection spread by mosquitoes.",
    "description_ta": "கொசுக்களால் பரவும் மூளை தொற்று.",
    "first_aid_en": [
      "Reduce fever with sponging.",
      "Keep patient safe during fits.",
      "Hospitalize immediately."
    ],
    "first_aid_ta": [
      "காய்ச்சலை குறைக்க ஈரத்துணியால் துடைக்கவும்.",
      "வலிப்பின் போது பாதுகாக்கவும்.",
      "உடனே மருத்துவமனையில் சேர்க்கவும்."
    ],
    "when_to_see_doctor_en": "IMMEDIATELY (Emergency).",
    "when_to_see_doctor_ta": "உடனடியாக (அவசரம்)."
  },
  "Leptospirosis": {
    "title_en": "Leptospirosis (Rat Fever)",
    "title_ta": "எலிக்காய்ச்சல்",
    "description_en": "Bacterial infection from animal urine in water.",
    "description_ta": "விலங்கு சிறுநீர் கலந்த நீரால் வரும் தொற்று.",
    "first_aid_en": [
      "Wash limbs with soap/Turmeric water after mud contact.",
      "Rest and hydrate.",
      "Monitor eyes."
    ],
    "first_aid_ta": [
      "சேற்றில் பட்ட இடங்களை சோப்பு அல்லது மஞ்சள் நீரால் கழுவவும்.",
      "ஓய்வு எடுக்கவும்.",
      "கண்களை கண்காணிக்கவும்."
    ],
    "when_to_see_doctor_en": "If red eyes/muscle pain occur.",
    "when_to_see_doctor_ta": "கண் சிவத்தல்/தசை வலி இருந்தால்."
  },
  "Typhoid Fever": {
    "title_en": "Typhoid Fever",
    "title_ta": "டைபாய்டு காய்ச்சல்",
    "description_en": "Water-borne bacterial infection causing sustained fever.",
    "description_ta": "அசுத்த நீர் மூலம் பரவும் தொடர் காய்ச்சல் நோய்.",
    "first_aid_en": [
      "Drink boiled and cooled water.",
      "Eat soft food (Porridge/Idli).",
      "Maintain hygiene."
    ],
    "first_aid_ta": [
      "நன்கு காய்ச்சி ஆறவைத்த நீரை குடிக்கவும்.",
      "மென்மையான உணவு (கஞ்சி/இட்லி) உண்ணவும்.",
      "சுகாதாரம்."
    ],
    "when_to_see_doctor_en": "If fever > 3 days.",
    "when_to_see_doctor_ta": "காய்ச்சல் 3 நாட்களுக்கு மேல் இருந்தால்."
  },
  "Cholera": {
    "title_en": "Cholera",
    "title_ta": "காலரா",
    "description_en": "Severe watery diarrhea leading to rapid dehydration.",
    "description_ta": "கடுமையான நீர் வயிற்றுப்போக்கு மற்றும் நீர்ச்சத்து இழப்பு.",
    "first_aid_en": [
      "Start ORS immediately.",
      "Drink salted rice water or tender coconut.",
      "Breastfeed infants."
    ],
    "first_aid_ta": [
      "உடனே ORS குடிக்கவும்.",
      "உப்பு கஞ்சி அல்லது இளநீர் குடிக்கவும்.",
      "தாய்ப்பால் தொடரவும்."
    ],
    "when_to_see_doctor_en": "IMMEDIATELY.",
    "when_to_see_doctor_ta": "உடனடியாக."
  },
  "Acute Gastroenteritis": {
    "title_en": "Acute Gastroenteritis",
    "title_ta": "வயிற்றுப்போக்கு மற்றும் வாந்தி",
    "description_en": "Stomach infection causing vomiting and diarrhea.",
    "description_ta": "வாந்தி மற்றும் வயிற்றுப்போக்கை ஏற்படுத்தும் வயிற்று தொற்று.",
    "first_aid_en": [
      "Hydrate often.",
      "Eat bland diet (Curd rice).",
      "Avoid milk."
    ],
    "first_aid_ta": [
      "அடிக்கடி நீர் அருந்தவும்.",
      "தயிர் சாதம் போன்ற எளிய உணவு.",
      "பால் தவிர்க்கவும்."
    ],
    "when_to_see_doctor_en": "If dehydration signs appear.",
    "when_to_see_doctor_ta": "நீர்ச்சத்து குறைவு அறிகுறிகள் இருந்தால்."
  },
  "Amoebic Dysentery": {
    "title_en": "Amoebic Dysentery",
    "title_ta": "சீதபேதி",
    "description_en": "Intestinal infection causing bloody/mucus diarrhea.",
    "description_ta": "ரத்தம்/சளி கலந்த வயிற்றுப்போக்கை ஏற்படுத்தும் குடல் தொற்று.",
    "first_aid_en": [
      "Drink fluids.",
      "Maintain hygiene.",
      "Medical treatment needed."
    ],
    "first_aid_ta": [
      "திரவம் அருந்தவும்.",
      "சுகாதாரம்.",
      "மருத்துவ சிகிச்சை தேவை."
    ],
    "when_to_see_doctor_en": "Always requires antibiotics.",
    "when_to_see_doctor_ta": "எப்போதும் மருந்து தேவை."
  },
  "Bacillary Dysentery": {
    "title_en": "Bacillary Dysentery",
    "title_ta": "ரத்த பேதி",
    "description_en": "Bacterial infection causing severe bloody diarrhea.",
    "description_ta": "கடுமையான ரத்த பேதியை ஏற்படுத்தும் பாக்டீரியா தொற்று.",
    "first_aid_en": [
      "Drink ORS.",
      "Hydrate.",
      "See doctor."
    ],
    "first_aid_ta": [
      "ORS குடிக்கவும்.",
      "நீர் அருந்தவும்.",
      "மருத்துவரை பார்க்கவும்."
    ],
    "when_to_see_doctor_en": "Immediately.",
    "when_to_see_doctor_ta": "உடனடியாக."
  },
  "Hepatitis A": {
    "title_en": "Hepatitis A",
    "title_ta": "மஞ்சள் காமாலை (A)",
    "description_en": "Liver infection causing jaundice.",
    "description_ta": "மஞ்சள் காமாலையை ஏற்படுத்தும் கல்லீரல் தொற்று.",
    "first_aid_en": [
      "Rest completely.",
      "Eat strict fat-free diet (Keezhanelli herbal remedy often used).",
      "Drink water."
    ],
    "first_aid_ta": [
      "முழு ஓய்வு.",
      "கொழுப்பு இல்லாத உணவு (கீாநெல்லி அதிகம் பயன்படுத்துவர்).",
      "நீர் அருந்தவும்."
    ],
    "when_to_see_doctor_en": "For diagnosis and monitoring.",
    "when_to_see_doctor_ta": "பரிசோதனைக்கு."
  },
  "Hepatitis E": {
    "title_en": "Hepatitis E",
    "title_ta": "மஞ்சள் காமாலை (E)",
    "description_en": "Water-borne liver infection.",
    "description_ta": "நீர் மூலம் பரவும் கல்லீரல் தொற்று.",
    "first_aid_en": [
      "Rest.",
      "Drink boiled water only.",
      "Avoid alcohol."
    ],
    "first_aid_ta": [
      "ஓய்வு.",
      "காய்ச்சிய நீர் மட்டும்.",
      "மது தவிர்க்கவும்."
    ],
    "when_to_see_doctor_en": "If pregnant (high risk).",
    "when_to_see_doctor_ta": "கர்ப்பிணியாக இருந்தால் (ஆபத்து)."
  },
  "Giardiasis": {
    "title_en": "Giardiasis",
    "title_ta": "குடல் தொற்று (Giardia)",
    "description_en": "Parasitic infection causing greasy diarrhea.",
    "description_ta": "பிசுபிசுப்பான வயிற்றுப்போக்கை ஏற்படுத்தும் ஒட்டுண்ணி.",
    "first_aid_en": [
      "Hydration.",
      "Hygiene.",
      "Wash hands thoroughly."
    ],
    "first_aid_ta": [
      "நீர்ச்சத்து.",
      "சுகாதாரம்.",
      "நன்கு கை கழுவுதல்."
    ],
    "when_to_see_doctor_en": "If diarrhea persists > 1 week.",
    "when_to_see_doctor_ta": "வயிற்றுப்போக்கு 1 வாரத்திற்கு மேல் இருந்தால்."
  },
  "Roundworm Infestation": {
    "title_en": "Roundworm",
    "title_ta": "உருளைப்புழு",
    "description_en": "Worms in intestine affecting nutrition.",
    "description_ta": "சத்துக்களை பாதிக்கும் குடல் புழுக்கள்.",
    "first_aid_en": [
      "Take de-worming medication.",
      "Maintain hygiene."
    ],
    "first_aid_ta": [
      "குடற்புழு நீக்க மருந்து எடுக்கவும்.",
      "சுகாதாரம்."
    ],
    "when_to_see_doctor_en": "For prescription (Albendazole).",
    "when_to_see_doctor_ta": "மருந்து சீட்டுக்கு."
  },
  "Hookworm Infection": {
    "title_en": "Hookworm",
    "title_ta": "கொக்கிப்புழு",
    "description_en": "Parasite entering through feet causing anemia.",
    "description_ta": "பாதம் வழியாக நுழைந்து ரத்த சோகையை ஏற்படுத்தும் புழு.",
    "first_aid_en": [
      "Eat iron-rich food (Spinach, Dates).",
      "Wear footwear always."
    ],
    "first_aid_ta": [
      "இரும்புச்சத்து உணவு (கீரை, பேரீச்சை) உண்ணவும்.",
      "எப்போதும் செருப்பு அணியவும்."
    ],
    "when_to_see_doctor_en": "For anemia treatment.",
    "when_to_see_doctor_ta": "ரத்த சோகை சிகிச்சைக்கு."
  },
  "Food Poisoning": {
    "title_en": "Food Poisoning",
    "title_ta": "உணவு நச்சு",
    "description_en": "Illness from contaminated food.",
    "description_ta": "கெட்டுப்போன உணவால் வரும் நோய்.",
    "first_aid_en": [
      "Rest stomach.",
      "Drink ginger tea or warm water.",
      "Sip fluids."
    ],
    "first_aid_ta": [
      "வயிற்றுக்கு ஓய்வு.",
      "இஞ்சி டீ அல்லது வெந்நீர் குடிக்கவும்.",
      "சிறிது நீர் அருந்தவும்."
    ],
    "when_to_see_doctor_en": "If bloody vomit/stools.",
    "when_to_see_doctor_ta": "ரத்த வாந்தி/பேதி இருந்தால்."
  },
  "Common Cold": {
    "title_en": "Common Cold",
    "title_ta": "சளி / ஜலதோஷம்",
    "description_en": "Viral nose/throat infection.",
    "description_ta": "மூக்கு/தொண்டை வைரஸ் தொற்று.",
    "first_aid_en": [
      "Steam inhalation with Turmeric.",
      "Drink warm ginger/pepper water.",
      "Rest."
    ],
    "first_aid_ta": [
      "மஞ்சள் கலந்து ஆவி பிடிக்கவும்.",
      "இஞ்சி/மிளகு பால் குடிக்கவும்.",
      "ஓய்வு."
    ],
    "when_to_see_doctor_en": "If breathing hard.",
    "when_to_see_doctor_ta": "மூச்சு விட சிரமப்பட்டால்."
  },
  "Influenza": {
    "title_en": "Influenza (Flu)",
    "title_ta": "ஃப்ளூ காய்ச்சல்",
    "description_en": "Viral infection with high fever and aches.",
    "description_ta": "அதிக காய்ச்சல், உடல் வலி தரும் வைரஸ்.",
    "first_aid_en": [
      "Rest.",
      "Drink plenty of fluids.",
      "Take Paracetamol."
    ],
    "first_aid_ta": [
      "ஓய்வு.",
      "நிறைய திரவங்கள் குடிக்கவும்.",
      "பாராசிட்டமால் எடுக்கவும்."
    ],
    "when_to_see_doctor_en": "High fever > 3 days.",
    "when_to_see_doctor_ta": "அதிக காய்ச்சல் 3 நாட்களுக்கு மேல்."
